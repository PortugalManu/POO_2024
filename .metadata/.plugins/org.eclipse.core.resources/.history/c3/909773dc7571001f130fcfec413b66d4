package ad002;

import java.util.Scanner;

public class ad002
{

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub

		Scanner sc = new Scanner(System.in);
		
		int A = sc.nextInt();
		int B = sc.nextInt();
		int C = sc.nextInt();
		int D = sc.nextInt();
		int DIF = (A * B - C * D);
		
		System.out.println("A direfença entre eles é: "+DIF);
		
		
	}

}